			Call pGenXMIDocumentacion( unaDescripcion, 4)

			Print #2, fInd(3) & fQ2q( "</productos>")
		End If

	Next anIndex
    
	Print #2, fInd(2) & fQ2q( "</productos>")

End Sub



Sub pGenXMITecnicas
	Dim anIndex As Integer

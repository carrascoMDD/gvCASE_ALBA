	If unaActividadIndex < 0 Then
		Print 
		Print
		Print "!!!!!!!!!!!!!!!!!!!!!!!!!!!!!"
		Print "Actividad " & theCodigoProceso & " " & theCodigoActividad & " not found!!!"
		Print
		Print
	End If

	unaTareaIndex = gNumTareas
	gNumTareas = gNumTareas + 1

	If gActividades_Tareas_count(unaActividadIndex) < 1 Then

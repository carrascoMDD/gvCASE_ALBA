        <operaciones xsi:type="es.gva.cit.simapa:Decision" titulo="¿ Hay antecedentes Mastín ?" numeroPagina="1" numeroOrden="7" flujosEntrantes="//@procedimientos/@procedimientos.0/@operaciones/@operaciones.4/@flujosSalientes.1 //@procedimientos/@procedimientos.0/@operaciones/@operaciones.5/@flujosSalientes.0" unidad="//@unidadesOrganicas/@descomposicion.0/@descomposicion.1">

            <flujos xsi:type="es.gva.cit.gvmetrica.proinso:Entrada" producto="//@metodologias.0/@productos/@productos.266" salidas="//@metodologias.0/@procesos/@procesos.2/@actividades.0/@tareas.6/@flujos.4" precondiciones="DSI 1.7"/>
            <flujos xsi:type="es.gva.cit.gvmetrica.proinso:Entrada" producto="//@metodologias.0/@productos/@productos.267" salidas="//@metodologias.0/@procesos/@procesos.2/@actividades.0/@tareas.6/@flujos.3" precondiciones="DSI 1.7"/>
            <flujos xsi:type="es.gva.cit.gvmetrica.proinso:Entrada" producto="//@metodologias.0/@productos/@productos.268" salidas="//@metodologias.0/@procesos/@procesos.1/@actividades.1/@tareas.0/@flujos.2" precondiciones="CSI 2.1"/>
            <flujos xsi:type="es.gva.cit.gvmetrica.proinso:Salida" producto="//@metodologias.0/@productos/@productos.268" entradas="//@metodologias.0/@procesos/@procesos.1/@actividades.2/@tareas.1/@flujos.0 //@metodologias.0/@procesos/@procesos.1/@actividades.5/@tareas.0/@flujos.1"/>
